{
  "extends": [
    "pixie",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-trailing-spaces": "error",
    "react/prop-types": "off"
  }
}